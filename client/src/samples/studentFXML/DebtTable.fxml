<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="tableOfDebits" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox>
               <children>
                  <TableView fx:id="subjectTable" prefHeight="388.0" prefWidth="84.0">
                    <columns>
                      <TableColumn fx:id="subjectCollom" prefWidth="83.0" text="Subject" />
                    </columns>
                  </TableView>
                  <TableView fx:id="debtTable" prefHeight="593.0" prefWidth="311.0">
                    <columns>
                      <TableColumn fx:id="numberOfWorkCollom" prefWidth="62.0" text="â„– work" />
                      <TableColumn fx:id="nameOfWorkCollom" prefWidth="145.0" text="Name of work" />
                        <TableColumn fx:id="lastDateCollom" prefWidth="103.0" text="Last date" />
                    </columns>
                  </TableView>
               </children>
            </HBox>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                  <Button fx:id="watchWork" mnemonicParsing="false" onAction="#watchWorkAction" text="Watch work" />
                  <Button fx:id="stayInQueue" mnemonicParsing="false" onAction="#stayInQueueAction" text="Stay in queue" />
              </buttons>
               <VBox.margin>
                  <Insets right="5.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
